<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="ResourceDictionaryName">Loc-tr-TR</sys:String>

    <sys:String x:Key="Title">Seçenekler</sys:String>

    <!-- Checkboxes -->
    <sys:String x:Key="Verify">Başlangıçta dosyaları otomatik olarak doğrula.</sys:String>
    <sys:String x:Key="Update">Güncelleme mevcutsa otomatik olarak güncelle.</sys:String>
    <sys:String x:Key="Verify_Threads">Eşzamanlı Kontrol Sayısı (En Az: 20, En Fazla: 20).</sys:String>
    <sys:String x:Key="Update_Threads">Eşzamanlı İndirme Sayısı (En Az: 4, En Fazla: 20).</sys:String>

    <!-- Button -->
    <sys:String x:Key="Name">İsim</sys:String>
    <sys:String x:Key="Default">Varsayılan</sys:String>
    <sys:String x:Key="Folder">Klasör Yolu</sys:String>
    <sys:String x:Key="Launcher">Başlatıcı Yolu</sys:String>
    <sys:String x:Key="Password">Şifre (Sadece BETA için geçerli)</sys:String>

    <!-- Others -->
    <sys:String x:Key="Find">Otomatik olarak indirilenleri bul</sys:String>
    <sys:String x:Key="Help">Yardım</sys:String>
    <sys:String x:Key="OK">Tamam</sys:String>
    <sys:String x:Key="Cancel">İptal</sys:String>

</ResourceDictionary>