<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="ResourceDictionaryName">Loc-en-GB</sys:String>

    <sys:String x:Key="Title">Edit Installation</sys:String>

    <!-- Table -->
    <sys:String x:Key="Name">Name</sys:String>
    <sys:String x:Key="Default">Default</sys:String>
    <sys:String x:Key="Folder">Path to module folder</sys:String>
    <sys:String x:Key="Launcher">Path to launcher</sys:String>
    <sys:String x:Key="Password">Password (for NI Betas only)</sys:String>

    <!-- Button -->
    <sys:String x:Key="Browse">Browse...</sys:String>
    <sys:String x:Key="Help">Help</sys:String>
    <sys:String x:Key="OK">OK</sys:String>
    <sys:String x:Key="Cancel">Cancel</sys:String>

    <!-- File Finder -->
    <sys:String x:Key="Warband_Modules">Select the Warband Modules folder</sys:String>
    <sys:String x:Key="Select_Steam">Select the Steam executable</sys:String>
    <sys:String x:Key="Select_Warband">Select the Warband executable</sys:String>
    <sys:String x:Key="Executables">Executables</sys:String>

</ResourceDictionary>