    <!-- Button -->
    <sys:String x:Key="B_verify">Controlla i file</sys:String>
    <sys:String x:Key="B_verifyProgress">Controllo dei file...</sys:String>
    <sys:String x:Key="B_update">Aggiorna i file</sys:String>
    <sys:String x:Key="B_updateProgress">In aggiornamento...</sys:String>
    <sys:String x:Key="B_play">Giocare</sys:String>

    <!-- Others -->
    <sys:String x:Key="ActiveInstall">Installazione Attiva:</sys:String>
    <sys:String x:Key="ForumLink">Apri thread del forum...</sys:String>
    <sys:String x:Key="NewsLoading">Caricamento delle ultime notizie...</sys:String>
