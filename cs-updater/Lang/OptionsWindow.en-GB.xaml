<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    
    <sys:String x:Key="ResourceDictionaryName">Loc-en-GB</sys:String>

    <sys:String x:Key="Title">Options</sys:String>

    <!-- Checkboxes -->
    <sys:String x:Key="Verify">Automatially verify files on startup.</sys:String>
    <sys:String x:Key="Update">Automatially update if update is available.</sys:String>
    <sys:String x:Key="Verify_Threads">Number of concurrent checks (Default: 20, Max: 20).</sys:String>
    <sys:String x:Key="Update_Threads">Number of concurrent downloads (Default: 4, Max: 20).</sys:String>

    <!-- Button -->
    <sys:String x:Key="Name">Name</sys:String>
    <sys:String x:Key="Default">Default</sys:String>
    <sys:String x:Key="Folder">Path to folder</sys:String>
    <sys:String x:Key="Launcher">Path to launcher</sys:String>
    <sys:String x:Key="Password">Password (for NI Betas only)</sys:String>

    <!-- Others -->
    <sys:String x:Key="Find">Automatically Find Installations</sys:String>
    <sys:String x:Key="Help">Help</sys:String>
    <sys:String x:Key="OK">OK</sys:String>
    <sys:String x:Key="Cancel">Cancel</sys:String>

</ResourceDictionary>